    <div class="stats-container">
      <div class="statistics">
        <div class="quantity">C.I</div>
        <div class="units">1753491214</div>
      
    </div>

      <div class="stats-container">
        <div class="statistics">
          <div class="quantity">Especialidad</div>
          <div class="units">Pedriatia</div>
        </div>
      </div>

        <div class="stats-container">
          <div class="statistics">
            <div class="quantity">Nombre del hospital</div>
            <div class="units">Baca Ortiz</div>
        </div>
        </div>
    </div>
  </div>
  
  <div class="attribution">
    Aqui <a href="http://127.0.0.1:5500/index.html" target="_blank">Lista de pacientes</a>. 
